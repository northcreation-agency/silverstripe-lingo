{
    "name": "northcreationagency/lingo",
    "description": "Handle yml lang texts in SilverStripe admin",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/northcreation-agency/silverstripe-lingo",
    "keywords": ["silverstripe", "language", "translation", "yml"],
    "authors": [
        {
            "name": "Emil Berg",
            "email": "emil@northcreation.agency"
        }
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/northcreation-agency/lingo/issues"
    },
    "require": {
        "silverstripe/cms": "^4",
        "ext-json": "*",
        "muskie9/data-to-arraylist": "2.0.0-beta2",
        "symfony/yaml": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "NorthCreationAgency\\SilverStripeLingo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NorthCreationAgency\\SilverStripeLingo\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
