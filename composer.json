{
    "name": "northcreationagency/silverstripe-lingo",
    "description": "Translate and handle keys from yml-files from within the SilverStripe admin.",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/northcreation-agency/silverstripe-lingo",
    "keywords": [
        "silverstripe",
        "language",
        "translation",
        "yml"
    ],
    "authors": [
        {
            "name": "Emil Berg",
            "email": "emil@northcreation.agency"
        }
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/northcreation-agency/silverstripe-lingo/issues"
    },
    "require": {
        "silverstripe/framework": "4.5.0@stable",
        "silverstripe/admin": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "NorthCreationAgency\\SilverStripeLingo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NorthCreationAgency\\SilverStripeLingo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "lingo"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
